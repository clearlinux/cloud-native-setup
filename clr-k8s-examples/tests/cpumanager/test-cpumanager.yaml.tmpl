---
apiVersion: v1
kind: Pod
metadata:
  name: test-cpumanager-guaranteed-__runtimeclass__
spec:
  restartPolicy: Never
  containers:
  - name: busy
    image: busybox
    command: [ "top" ]
    resources:
      limits:
        cpu: 1
        memory: 100Mi
